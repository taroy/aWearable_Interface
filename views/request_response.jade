script
    <!--Load accept- Ingrid-->
    
    function loadInvitationAnswer() {
        if (document.answerInvitation.acc1[0].checked) {
            document.getElementById("accept").innerHTML = "You accepted the invitation. Ingrid is now aware of you";
        }
        else if (document.answerInvitation.acc1[1].checked) {
            document.getElementById("accept").innerHTML = "You ignored the invitation.";
        }
    }
header.jumbotron.subhead#overview
      
      h1 aWearable
      
        h3 Ingrid has invited you to join her awareness group/Let Ingrid be aware of you
        <div id="accept">
        <form name="answerInvitation">
        <input type="radio" id = "acc1" name="acc1" onclick = "loadInvitationAnswer()" value="Accept" style=display:none> 
        <label for="acc1"><img src="accept.png" alt="Accepting" /></label>
        <input type="radio" id = "acc2" name="acc1" onclick = "loadInvitationAnswer()" value="Ignore" style=display:none>
        <label for="acc2"><img src="ignore.png" alt="Ignoring" /></label>
        
        </form>
        </div>
